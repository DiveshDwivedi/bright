{
    "name": "sankar/laravel-karla",
    "description": "Framework extension",
    "keywords": [
        "laravel",
        "permission",
        "acl",
        "security"
    ],
    "homepage": "https://github.com/sankarsuda/laravel-karla",
    "license": "MIT",
    "authors": [
        {
            "name": "Sankar Suda",
            "email": "sankar.suda@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.1",
        "laravel/framework": "^5.6",
        "spatie/laravel-permission": "^2.12",
        "schuppo/password-strength": "^1.11"
    },
    "require-dev": {
        "orchestra/testbench": "~3.3.0|~3.4.2|^3.5.0",
        "phpunit/phpunit": "^5.7|6.2|^7.0",
        "xethron/migrations-generator": "^2.0",
        "bpocallaghan/generators": "^5.0"
    },
    "suggest": {
        "portphp/csv": "^1.1",
        "portphp/excel": "^1.1",
        "portphp/portphp": "^1.3",
        "piwik/device-detector": "^3.10",
        "willdurand/geocoder": "^4.2",
        "geocoder-php/chain-provider": "^4.0",
        "geocoder-php/common-http": "^4.0",
        "geocoder-php/free-geoip-provider": "^4.0",
        "geocoder-php/geoip2-provider": "^4.0",
        "geocoder-php/host-ip-provider": "^4.0",
        "geoip2/geoip2": "^2.9",
        "consoletvs/charts": "^6.2"
    },
    "autoload": {
        "psr-4": {
            "Karla\\": "src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Karla\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Karla\\KarlaServiceProvider"
            ]
        }
    }
}