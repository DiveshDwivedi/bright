{
    "name": "sankar/laravel-karla",
    "description": "Framework extension",
    "keywords": [
        "laravel",
        "permission",
        "acl",
        "security"
    ],
    "homepage": "https://github.com/sankarsuda/laravel-karla",
    "license": "MIT",
    "authors": [
        {
            "name": "Sankar Suda",
            "email": "sankar.suda@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.0",
        "laravel/framework": "5.6.*",
        "spatie/laravel-permission": "^2.12"
    },
    "require-dev": {
        "orchestra/testbench": "~3.3.0|~3.4.2|^3.5.0",
        "phpunit/phpunit": "^5.7|6.2|^7.0",
        "xethron/migrations-generator": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Karla\\": "src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Karla\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Karla\\KarlaServiceProvider"
            ]
        }
    }
}